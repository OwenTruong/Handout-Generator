{
  "name": "handout-generator",
  "version": "1.0.0",
  "description": "Handout Generator package that has a Handout class that exposes a createHandout method for generating handout pdfs from jpg, png and/or pdf.",
  "main": "./lib/main.js",
  "scripts": {
    "build": "webpack && chmod +x bin/main.mjs"
  },
  "keywords": [
    "Handout",
    "PDF",
    "Notes"
  ],
  "author": "Owen Truong",
  "license": "MIT",
  "typings": "./lib/**/*.d.ts",
  "bin": {
    "handgen": "./bin/main.mjs"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.3",
    "@babel/preset-typescript": "^7.18.6",
    "babel-loader": "^8.2.5",
    "babel-plugin-ramda": "^2.1.1",
    "webpack": "^5.74.0"
  },
  "dependencies": {
    "libreoffice-convert": "^1.4.1",
    "pdf-lib": "^1.17.1",
    "ts-loader": "^9.4.2",
    "webpack-cli": "^4.10.0"
  }
}
