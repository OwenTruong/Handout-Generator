{
  "compilerOptions": {
    "target": "ES2020",

    "module": "CommonJS",

    "esModuleInterop": true,

    "forceConsistentCasingInFileNames": true,

    "strict": true,

    "skipLibCheck": true,

    "baseUrl": "src",

    "paths": {
      "@/*": ["CreatePDFModule/*"],
      "@classes/*": ["CreatePDFModule/classes/*"],
      "@constants/*": ["CreatePDFModule/constants/*"],
      "@defaults/*": ["CreatePDFModule/defaults/*"],
      "@functions/*": ["CreatePDFModule/functions/*"]
    }
  }
}
